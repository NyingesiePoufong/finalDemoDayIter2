<!doctype html>
<html>
<head>
    <title>Profile/Dashboard</title>
    <link rel="stylesheet" href="//netdna.bootstrapcdn.com/bootstrap/3.0.2/css/bootstrap.min.css">
    <link rel="stylesheet" href="//netdna.bootstrapcdn.com/font-awesome/4.0.3/css/font-awesome.min.css">
    <link rel="stylesheet" href="style.css">
</head>
<body>
<div class="container">
  <% if (user.local.role === "teacher") { %>
      <%- include("teacherProfile", {user:user, quizList:quizList}) %>
<%  } else if (user.local.role === "student") { %>
      <%- include("studentProfile", {user:user, assignments:assignments}) %>
<% } else if (user.local.role === "parent") { %>
    <%- include("parentProfile", {user:user}) %>
<% }  %>

    <div class="page-header text-center">
        <a href="/logout" class="btn btn-default btn-sm">Logout</a>
    </div>


</body>
</html>
