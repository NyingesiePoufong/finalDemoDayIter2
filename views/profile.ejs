    <link rel="stylesheet" href="style.css">
</head>
<body>

    <nav class="navbar navbar-expand-lg navbar-dark bg-info mb-3">
    <a class="navbar-brand text-white" href="/">Balance Ed</a>

    <a href="/logout" class="nav-link text-white ml-auto">Logout</a>
    <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarColor01" aria-controls="navbarColor01" aria-expanded="false" aria-label="Toggle navigation">
    <span class="navbar-toggler-icon"></span>
    </button>


    </nav>

  <% if (user.local.role === "teacher") { %>
      <%- include("teacherProfile", {user:user, quizList:quizList}) %>
  <%  } else if (user.local.role === "student") { %>
      <%- include("studentProfile", {user:user, assignments:assignments}) %>
  <% } else if (user.local.role === "parent") { %>
    <%- include("parentProfile", {user:user}) %>
  <% }  %>

  


</body>
</html>
