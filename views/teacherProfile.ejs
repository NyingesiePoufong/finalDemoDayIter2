  </head>
  <body>
    <h1>I'm A Teacher</h1>
    <br>

    <a href="/createQuiz"><button type="button" name="button" class="btn btn-dark">Create New Quiz</button></a>

    <br>
    <br>
    <br>


      <div class="list-group">
        <% for(var i = 0; i < quizList.length; i++) { %>
          <a class="list-group-item list-group-item-action" href="/gradeQuiz?quizId=<%=quizList[i]._id%>&course=<%=quizList[i].course%>&quizName=<%= quizList[i].quizName %>"><%= quizList[i].quizName %></a>
        <% } %>
      </div>
  </body>
</html>
